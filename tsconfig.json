{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "lib": [
      "ES2019.Array",
      "ES2020.Promise",
      "ES2015"
    ]
  },
  "include": [
    "src"
  ]
}